   <!--部分テンプレート-->
   <%= render 'books/list', book: @book, user: @user %>

   <h3>Users</h3>
    <!--table-->
    <table>
      <thead>
        <tr><!--1行目の見出し-->
          <th><strong>image</strong></th>
          <th><strong>name</strong></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%=image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>


